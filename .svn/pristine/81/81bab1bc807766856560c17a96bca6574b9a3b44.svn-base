<script>
    import api from '../../common/api.js';
    
	export default {
		data() {
			return {
				state:0,     //0未知      1、未授权           2、未绑定       3、已登录
                community:null,
                starNum:0,
                mask:false,
                
                searchCommunityid:null
			}
		},
        onShow() {
            if(this.searchCommunityid){
                this.findCommunity(this.searchCommunityid)
            }
        },
		onLoad() {
            var _self = this
            
            uni.setNavigationBarColor({
                frontColor: '#ffffff',
                backgroundColor: '#F6F9FF',
               
            })
            
            uni.checkSession({
                success() {
                    // session_key 未过期，并且在本生命周期一直有效
                    console.log("未过期");
                    _self.wxlogin()
                        
                },
                fail() {
                    console.log("过期");
                    _self.wxlogin()
                }
            })
            
            
           
		},
		methods: {
            clickDoctor:function(){
                if(!this.isShowjoin()){
                    return
                }
                uni.navigateTo({
                    url:"doctorInfo?doctorid="+this.community.expert.id
                })
            },
            maskCancel:function(){
                this.mask = false
            },
            clickJoin:function(){
                
            },
            isShowjoin:function(){
                if(!this.community.join){
                    this.mask = true
                    return false
                }
                return true
            },
            imageInterrogation:function(){
                if(!this.isShowjoin()){
                    return
                }
                var data = {
                    expertId:this.community.expert.id,
                    type:'IMAGE',
                    communityId:this.community.id
                }
                api.addAdvisoryRecord({
                    data:data
                }).then(res=>{
                    console.log(res)
                })
            },
            videoInterrogation:function(){
                if(!this.isShowjoin()){
                    return
                }
                var data = {
                    expertId:this.community.expert.id,
                    type:'VIDEO',
                    communityId:this.community.id
                }
                api.addAdvisoryRecord({
                    data:data
                }).then(res=>{
                    console.log(res)
                    if(res.status == 'OK'){
                        uni.navigateTo({
                            url:"connectVideoInterrogation?recordId=" + res.data.id +"&to=" + this.community.expert.id
                        })
                    }
                })
                
            },
            clickAppraise:function(){
                if(!this.isShowjoin()){
                    return
                }
                if(this.starNum == 0){
                    uni.showToast({
                        title: "请先选择评分",
                        icon: 'none',
                        duration: 2000
                    })
                    return
                }
                var data = {
                    "communityId": this.community.id,
                    "score": this.starNum
                }
                
                api.addCommunityAppraise({
                    data:data
                }).then(res=>{
                    console.log(res)
                    
                })
            },
            clickStar:function(index,tag){
                if(!this.isShowjoin()){
                    return
                }
             
                if(tag){
                    if(this.starNum && index == 0){
                        this.starNum = 0
                    }else{
                        this.starNum = index + 1
                    }
                    
                }else{
                    this.starNum = this.starNum + index + 1
                }
                console.log(this.starNum)
            },
            wxlogin:function(){
                var _self = this
                uni.login({
                    provider: 'weixin',
                    success: function(loginRes) {
                        console.log(loginRes)
                        const obj = {
                            code: loginRes.code
                        }
                        api.getUnionid({
                            data: obj
                        }).then(res => {
                            console.log(res)
                            console.log('sk:' + res.data.session_key)
                            uni.setStorageSync('sk', res.data.session_key);
                            uni.setStorageSync('openid', res.data.openid);
                            uni.setStorageSync('unionid', res.data.unionid);
                            _self.checkAuth()
                        })
                    }
                });
            },
            checkAuth: function() {
                var _self = this
                uni.getUserInfo({
                    success(res) { //已经授权请求是否绑定信息
                        
                        _self.getAccount(res)
                    },
                    fail(res) { //没有授权直接跳转登录
                        console.log("没有授权")
                        _self.state = 1
                    }
                })
            },
            getAccount: function(res) {
                const sessionKey = uni.getStorageSync('sk');
                // console.log("res.iv:")
                // console.log(res.iv)
                const data = {
                    encryptedData: res.encryptedData,
                    iv: res.iv,
                    sessionKey: sessionKey
                }
                var _self = this
                api.getAccountInfo({
                    data: data
                }).then(accountRes => {
                    console.log("accountRes:")
                    console.log(accountRes)
                    if (!accountRes.data.bnUserInfo) {
                        uni.removeStorageSync('sk');
                        return;
                    }
                    
                    if(accountRes.data.wxUserInfo && accountRes.data.wxUserInfo.unionid ){
                             
                        uni.setStorageSync('unionid', accountRes.data.wxUserInfo.unionid);
                        
                    }
                    if(accountRes.data.bnUserInfo && accountRes.data.bnUserInfo.sessionId ){
                        const sessionId = accountRes.data.bnUserInfo.sessionId
                        uni.setStorageSync('sessionId', sessionId)
                    }
                    
                    // uni.setStorageSync('sessionId', 'b477011c669c4770812174d335c8226a')
                    // try{
                    if (accountRes.data.bnUserInfo.phone == "" || accountRes.data.bnUserInfo.phone == null) { //未绑定
                        this.state = 2
                        uni.navigateTo({
                            url:'login'
                        })
                    }else{
                        // uni.setStorageSync('userinfo', accountRes.data.bnUserInfo);
                        this.findUser(accountRes.data.bnUserInfo.id)
                    }
                })
            },        
            getUserList: function(res) {
                console.log(res)
                 if (this.login) {
                     return
                 }
                if (res.detail.errMsg == "getUserInfo:ok") {
                    //授权
                    if (res.iv) {
                        this.getAccount(res)
                    } else {
                        this.getAccount(res.target)
                    }
                
                } else {
                    //拒绝授权
                    uni.showModal({
                        title: '提示',
                        content: '允许授权后放可使用',
                        success: function(res) {
                            if (res.confirm) {
                                uni.openSetting({
                                    success(openRes) {
                                        console.log(res.authSetting)
                                    }
                                });
                            } else if (res.cancel) {
                
                            }
                        }
                    });
                
                }
            },
            findUser:function(id){
                const userObj = {
                    userId: id,
                }
                api.findUserById({
                    data:userObj
                }).then(userRes => {
                    console.log(userRes)
                    if(userRes.status == 'OK'){
                        uni.setStorageSync('userinfo', userRes.data)
                        if( userRes.data && userRes.data.followCommunityId && userRes.data.followCommunityId.length > 3 ){
                            this.emchatLogin(id)
                            this.findCommunity(userRes.data.followCommunityId)
                            
                        }else{      //跳转加入服务站
                            
                        }
                    }
                })
            },
            emchatLogin:function(userid){
                
                var pwd = userid.substring(1,userid.length-1)
                this.$conn.open({
                	apiUrl: this.$im.config.apiURL,
                	user: userid,
                	pwd: pwd,
                	grant_type: "password",
                	appKey: this.$im.config.appkey
                });
            },
            findCommunity:function(id){
                const data = {
                    id:id
                }
                api.findCommunityById({
                    data:data
                }).then(res => {
                    console.log(res)
                    if(res.status == 'OK'){
                        this.community = res.data
                        this.community.icon = JSON.parse(this.community.icon)[0].url
                        this.community.mangerAvatar = JSON.parse(this.community.mangerAvatar)[0].url
                        if(this.community.expert.pics && JSON.parse(this.community.expert.pics).length > 0){
                            this.community.expert.pics = JSON.parse(this.community.expert.pics)[0].url
                        }
                        
                        uni.setNavigationBarColor({
                            frontColor: '#ffffff',
                            backgroundColor: '#008A67',
                           
                        })
                        this.state = 3
                    }
                })
            },
            clickAL:function(tag){
                if(!this.isShowjoin()){
                    return
                }
                if(tag == 0){
                    uni.navigateTo({
                        url:'alskin/welcome?communityId='+this.community.id
                    })
                }else if(tag == 1){
                    uni.navigateTo({
                        url:'aldiscriminate/index'
                    })
                }else if(tag == 2){
                    uni.navigateTo({
                        url:'aldiscriminate/basicInfo?type=2'
                    })
                }
                
            }
		}
	}
</script>

