.list {
  margin-top: 10px;
  background: white;
  overflow: hidden;
}
.list .item .item-title2 {
  padding: 30px 15px 0;
  color: #16202E;
  font-size: 18px;
  line-height: 25px;
}
.list .item .item-img {
  font-size: 0;
  padding: 0;
  margin-bottom: 120rpx;
}
.list .item .item-img ._img {
  width: 100%;
}
.swiper-box {
  height: 750rpx;
}
.swiper-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  height: 750rpx;
}
.image {
  width: 100%;
  height: 100%;
}
.desc {
  background-color: #FFFFFF;
  padding: 30rpx 20rpx;
}
.health-list-title {
  font-size: 36rpx;
  font-weight: 800;
  color: #16202E;
}
.health-list-price {
  font-weight: 500;
  color: #03BE90;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.health-list-price text {
  font-size: 30rpx;
  position: relative;
  margin-left: 10rpx;
  color: #03BE90;
}
.health-list-price text:nth-child(1) {
  font-size: 28rpx;
}
.health-list-price text:nth-child(2) {
  font-size: 34rpx;
  font-weight: 800;
}
.health-list-price text:nth-child(3) {
  font-size: 28rpx;
  color: #A0A8BC;
}
.health-list-price text:nth-child(4) {
  font-size: 32rpx;
  color: #A0A8BC;
}
.health-list-price text:nth-child(4)::after {
  font-size: 24rpx;
  content: '';
  position: absolute;
  left: 0;
  top: 49%;
  width: 100%;
  height: 1px;
  background-color: #A0A8BC;
}
.detail-content {
  background-color: #FFFFFF;
  margin-top: 20rpx;
}
.goods-carts {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}

