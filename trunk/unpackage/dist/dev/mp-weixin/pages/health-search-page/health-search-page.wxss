.search-bar {
  background-color: #FFFFFF;
}
.search-content .search-list {
  background-color: #FFFFFF;
  border-radius: 30rpx;
  margin: 20rpx 32rpx;
  padding: 20rpx;
}
.search-content .search-list .search-list-pic {
  width: 160rpx;
  height: 160rpx;
  border-radius: 30rpx;
  overflow: hidden;
}
.search-content .search-list .search-list-pic image {
  width: 100%;
  height: 100%;
}
.search-content .search-list .search-list-info {
  padding-left: 30rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.search-content .search-list .search-list-info-realprice {
  color: #03BE90;
  font-weight: 500;
}
.search-content .search-list .search-list-info-realprice text:nth-child(1) {
  font-size: 20rpx;
}
.search-content .search-list .search-list-info-realprice text:nth-child(2) {
  font-size: 24rpx;
}
.search-content .search-list .search-list-info-price {
  font-size: 20rpx;
  font-weight: 500;
  color: #A0A8BC;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 8rpx;
  margin-left: 30rpx;
}
.search-content .search-list .search-list-info-price text {
  position: relative;
  margin-left: 20rpx;
}
.search-content .search-list .search-list-info-price text:nth-child(2)::after {
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  height: 1px;
  background-color: #A0A8BC;
}
.empty {
  padding: 30rpx 20rpx;
  color: #A2A9BA;
  font-size: 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.empty image {
  width: 20px;
  height: 20px;
}
.empty text {
  margin-left: 10rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}

