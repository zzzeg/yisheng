.upload {
  padding-bottom: 124rpx;
  overflow: hidden;
}
.upload .upload-header {
  height: 83rpx;
  background: white;
  -webkit-box-shadow: 0px 4px 21px 0px rgba(85, 112, 105, 0.1);
          box-shadow: 0px 4px 21px 0px rgba(85, 112, 105, 0.1);
  margin-top: 31rpx;
  padding-left: 33rpx;
  font-size: 29rpx;
}
.upload .upload-header text {
  font-family: PingFangSC-Regular, PingFang SC;
  margin-right: 81rpx;
  font-weight: 400;
  color: #16202e;
}
.upload .upload-content {
  margin-top: 31rpx;
  padding-top: 21rpx;
  padding-left: 33rpx;
  padding-right: 33rpx;
  padding-bottom: 70rpx;
  -webkit-box-shadow: 0px 4px 21px 0px rgba(85, 112, 105, 0.1);
          box-shadow: 0px 4px 21px 0px rgba(85, 112, 105, 0.1);
  background: white;
}
.upload .upload-content .upload-content-header {
  font-size: 29rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #16202e;
  line-height: 42rpx;
}
.upload .upload-content .upload-content-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.upload .upload-content .upload-content-list > view:nth-child(4n+1) {
  margin-left: 0;
}
.upload .upload-content .upload-content-list .upload-img {
  margin-top: 33rpx;
  margin-left: 32rpx;
  width: 146rpx;
  height: 146rpx;
  position: relative;
