}
.upload .upload-content .upload-content-list .upload-img image {
  width: 146rpx;
  height: 146rpx;
}
.upload .upload-content .upload-content-list .upload-img .delete {
  width: 31rpx;
  height: 31rpx;
  text-align: center;
  line-height: 26rpx;
