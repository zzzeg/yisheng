  right: 0;
  color: #FFFFFF;
  font-size: 25rpx;
}
.upload .upload-content .upload-content-list .add {
  margin-top: 33rpx;
  width: 146rpx;
  height: 146rpx;
  background: white;
  border: 2px dashed #383e56;
  color: #434E5E;
  font-size: 44rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.tips {
  width: 683rpx;
  margin: 0 auto;
  margin-top: 31rpx;
  font-size: 29rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #a2a9ba;
  line-height: 42rpx;
}
.done {
  width: 750rpx;
  height: 104rpx;
  background: -webkit-linear-gradient(217deg, #88e296 0%, #03be90 100%);
  background: linear-gradient(233deg, #88e296 0%, #03be90 100%);
  font-size: 33rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: white;
  position: fixed;
  left: 0;
  bottom: 0;
}
