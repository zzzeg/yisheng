
page{
    background: #EFF1F6;
}
.content{
    text-align: center;
}
.content image{
    width: 100rpx;
    height: 100rpx;
    margin-top: 180rpx;
}
.title{
    margin-top: 20rpx;
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(42,52,65,1);
    line-height:44rpx;
}
.ddd{
    margin-top: 12rpx;
    font-size:30rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(134,142,157,1);
    line-height:42rpx;
}
.login-btn{
    margin-top: 80rpx;
    height: 80rpx;
    width: calc(100% - 300rpx);
    border-radius: 45rpx;
    color: #FFFFFF;
    font-size: 31rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    background:-webkit-linear-gradient(217deg,rgba(136,226,150,1) 0%,rgba(3,190,144,1) 100%);
    background:linear-gradient(233deg,rgba(136,226,150,1) 0%,rgba(3,190,144,1) 100%);
    box-shadow:0px 6rpx 31rpx 0px rgba(3,190,144,0.3);
}

