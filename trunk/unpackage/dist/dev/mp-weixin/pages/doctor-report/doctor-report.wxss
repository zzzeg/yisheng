.record-item {
  margin: 30rpx;
  font-size: 28rpx;
  padding: 30rpx;
  border-radius: 10px;
  background-color: #FFFFFF;
  overflow: hidden;
}
.record-item .name {
  font-size: 33rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #16202e;
  line-height: 46rpx;
}
.record-item .doctor {
  color: #434E5E;
}
.record-item .time {
  color: #A2A9BA;
  margin-bottom: 12rpx;
}
.record-item .time image {
  width: 36rpx;
  height: 36rpx;
}
.record-item .edit {
  ont-size: 29rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #03be90;
  line-height: 42rpx;
}
.record-item .status {
  margin-top: 20rpx;
}
.record-item .status view:nth-child(2) {
  font-size: 24rpx;
  color: #FFFFFF;
  padding: 0 20rpx;
  background: -webkit-linear-gradient(135deg, #88e296 0%, #03be90 100%);
  background: linear-gradient(315deg, #88e296 0%, #03be90 100%);
  border-radius: 14px;
}
.upload-content-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.upload-content-list > view:nth-child(4n+1) {
  margin-left: 0;
}
.upload-content-list .upload-img {
  margin-top: 33rpx;
  margin-left: 15rpx;
  width: 146rpx;
  height: 146rpx;
  position: relative;
  overflow: hidden;
}
.upload-content-list .upload-img image {
  width: 146rpx;
  height: 146rpx;
}
.colorG {
  color: #03be90;
  font-size: 50rpx;
}
.done {
  width: 750rpx;
  height: 104rpx;
  background: #FFFFFF;
  font-size: 33rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #16202E;
  position: fixed;
  left: 0;
  bottom: 0;
}
.uni-tip {
  padding: 44rpx 65rpx;
  min-height: 250rpx;
  width: 563rpx;
  background: white;
  border-radius: 31rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.uni-tip .uni-tip-content {
  font-size: 31rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #6d7480;
  line-height: 46rpx;
}
.uni-tip .uni-tip-img {
  margin: 27rpx 0;
}
.uni-tip .uni-tip-img image {
  width: 138rpx;
  height: 129rpx;
}
.uni-tip .uni-tip-group-button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-align-self: stretch;
          align-self: stretch;
}
.uni-tip .uni-tip-group-button.center {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.uni-tip .uni-tip-group-button .uni-tip-button {
  width: 208rpx;
  height: 75rpx;
  line-height: 75rpx;
  background: -webkit-linear-gradient(217deg, #88e296 0%, #03be90 100%);
  background: linear-gradient(233deg, #88e296 0%, #03be90 100%);
  -webkit-box-shadow: 0px 6px 31px 0px rgba(3, 190, 144, 0.3);
          box-shadow: 0px 6px 31px 0px rgba(3, 190, 144, 0.3);
  border-radius: 38rpx;
  color: #FFFFFF;
  text-align: center;
}
.noreport {
  font-size: 33rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #434e5e;
  line-height: 46rpx;
}
.please {
  margin-top: 12rpx;
  font-size: 29rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #a2a9ba;
  line-height: 42rpx;
}

