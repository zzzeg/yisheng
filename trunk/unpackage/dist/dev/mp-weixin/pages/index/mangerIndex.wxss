page {
  background: #EFF1F6;
}
.content {
  text-align: center;
  font-size: 29rpx;
  color: #434E5E;
}
.logo {
  height: 264rpx;
  width: 100%;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  z-index: -99;
}
.login-box {
  width: 683rpx;
  height: 224rpx;
  /* margin-top: -310upx; */
  position: relative;
  margin-top: 125rpx;
  margin-left: 33rpx;
}
.boxxxx {
  position: absolute;
  z-index: -97;
  width: 100%;
  background: #FFFFFF;
  box-shadow: 0rpx 42rpx 208rpx 0rpx rgba(22, 32, 46, 0.15);
  border-radius: 31rpx;
  height: 220rpx;
  top: 84rpx;
}
.headerBox {
  width: 168rpx;
  height: 168rpx;
  margin-top: -84rpx;
  background: #FFFFFF;
  border-radius: 50%;
  margin-left: calc(50% - 84rpx);
}
.headerBox image {
  margin: 8rpx;
  width: 152rpx;
  height: 152rpx;
  border-radius: 50%;
  background: #F8F8F8;
}
.menuBox {
  margin: 150rpx 40rpx 0rpx 40rpx;
}
.menuText {
  width: 100%;
  text-align: left;
  height: 60rpx;
  padding-left: 10rpx;
}
.menuLine {
  width: 128rpx;
  height: 100%;
  border-radius: 3rpx;
  background: #03BE90;
}
.product {
  padding: 30rpx 36rpx;
}
.product .h-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.product .health-list {
  margin-bottom: 40rpx;
  padding-bottom: 16rpx;
  width: 320rpx;
  background-color: #FFFFFF;
  border-radius: 30rpx;
  overflow: hidden;
}
.product .health-list image {
  width: 320rpx;
  height: 310rpx;
}
.product .health-list-title {
  font-size: 24rpx !important;
  font-weight: 500;
  height: 2em;
  line-height: 2;
  overflow: hidden;
  color: #16202E;
  text-align: left;
  padding: 0 20rpx;
}
.product .health-list-price {
  font-weight: 500;
  color: #03BE90;
  margin-right: 10rpx;
}
.product .health-list-price view {
  font-size: 30rpx;
  margin-left: 10rpx;
}
.product .health-list-price view.price text {
  font-size: 20rpx;
  margin-right: 16rpx;
}
.product .health-list-price view:nth-child(2) text {
  margin-left: 16rpx;
  position: relative;
  font-size: 22rpx;
  color: #C6CAD4;
}
.product .health-list-price view:nth-child(2) text::after {
  font-size: 24rpx;
  content: '';
  position: absolute;
  left: 0;
  top: 49%;
  width: 100%;
  height: 1px;
  background-color: #A0A8BC;
}
.product > .h-wrap > .health-list > .health-list-title {
  font-size: 30rpx !important;
  font-weight: 500;
  line-height: 42rpx;
  margin: 10rpx 0 0 0;
}
.product > .h-wrap > .health-list > .health-list-price {
  text-align: left;
  padding: 0 20rpx;
}
.product > .h-wrap > .health-list > .health-list-price > view {
  margin: 0;
  line-height: 34rpx;
}
.imgList {
  margin-top: 60rpx;
}
.zhengjianimg {
  width: 60vw;
  height: 36vw;
}
.empty {
  text-align: center;
}
.empty .empty-img {
  margin-top: 52rpx;
  width: 360rpx;
  height: 320rpx;
}
.empty .empty-tips {
  color: #A2A9BA;
  font-size: 32rpx;
  line-height: 48rpx;
}

